<div class="container-fluid">
  <div class="row header">
    <div id="cos-sticker"class="col-lg-12">
      <span class="col-sm-2 img-responsive"><img src="http://i.imgur.com/7TvXM4L.jpg" alt="mason-sticker" /></span>
      <span class="col-sm-2 img-responsive"><img src="http://i.imgur.com/7TvXM4L.jpg" alt="mason-sticker" /></span>
      <span class="col-sm-2 img-responsive"><img src="http://i.imgur.com/7TvXM4L.jpg" alt="mason-sticker" /></span>
      <span class="col-sm-2 img-responsive"><img src="http://i.imgur.com/7TvXM4L.jpg" alt="mason-sticker" /></span>
      <span class="col-sm-2 img-responsive"><img src="http://i.imgur.com/7TvXM4L.jpg" alt="mason-sticker" /></span>
      <span class="col-sm-2 img-responsive"><img src="http://i.imgur.com/7TvXM4L.jpg" alt="mason-sticker" /></span>
    </div>
  </div>
  <div class="row">
    <div class="col-md-5">
      <nav id="nav" class="navbar navbar-default">
        <div class="container-fluid">
          <ul class="nav nav-pills red">
            <li id="music-link" role="presentation"><a href="#">Music</a></li>
            <li id="shows-link" role="presentation"><a href="#">Shows</a></li>
            <li id="shpadoinkle-link" role="presentation"><a href="#">Shpadoinkle</a></li>
            <li id="contact-link" role="presentation"><a href="#">Contact</a></li>
          </ul>
        </div>
      </nav>
    </div>
  </div>
  <div class="row welcome">
    <div class="row">
      <div class="col-md-4 col-md-offset-1">
        <p>
          We Are From Mason Cos
        </p>
      </div>
    </div>
    <div class="row">
      <div class="col-md-4 col-md-offset-1">
        <p>
          We Play Music
        </p>
      </div>
      <div class="col-md-4 email">
        <h4>Join our mailing list</h4>
        <span id="email_message"></span>
        <%= form_for(@user, remote: true) do |f| %>


          <%= f.label :email %>
          <%= f.email_field :email %>

          <%= f.submit 'Sign Up', class: 'btn btn-primary'%>
        <% end %>
      </div>
    </div>
  </div>

  <div id="music-section" class="row music">
    <div class="row soundcloud">
      <div class="col-sm-4 col-md-offset-1">
        <iframe width="100%" height="100" scrolling="no" frameborder="no" src="https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/tracks/224234846&amp;color=ff5500&amp;auto_play=false&amp;hide_related=false&amp;show_comments=true&amp;show_user=true&amp;show_reposts=false"></iframe>
      </div>
      <div class="col-sm-4 col-md-offset-1">
        <iframe width="100%" height="100" scrolling="no" frameborder="no" src="https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/tracks/224234806&amp;color=ff5500&amp;auto_play=false&amp;hide_related=false&amp;show_comments=true&amp;show_user=true&amp;show_reposts=false"></iframe>
      </div>
    </div>
    <div class="row youtube">
        <iframe width="550" height="315" src="https://www.youtube.com/embed/nvAD12G6-B4" frameborder="0" allowfullscreen></iframe>
    </div>
  </div>
  <div id="shows-section" class="row shows">
    <div class="col-md-6 col-md-offset-1">
      <div class="panel panel-default">
        <div class="panel-heading">
          <a href="https://www.facebook.com/events/961721757251828/"><h3>DTCV / Vamplifier (EP Release Show!) / Mason Cos</h3></a>
        </div>
        <div class="panel-body">
          <section class="col-sm-3">
            <a target="_blank" href="http://www.quenchers.com/"><p>Quencher's</p></a>
          </section>
          <section class="col-sm-3">
            <p>
              April 8, 2016
            </p>
          </section>
          <section class="col-sm-3">
            <a href="http://www.ticketweb.com/t3/sale/SaleEventDetail?eventId=6521055&pl=mp&dispatch=loadSelectionData">Tickets</a>
          </section>
        </div>
      </div>
    </div>
    <div class="col-md-4 col-md-offset-1 show_poster">

      <img id="poster" class="thumbnail" src="http://i.imgur.com/B3nAM6x.jpg?1" alt="show poster" />

      <div id="poster_modal" class="modal fade" tabindex="-1" role="dialog">
        <div class="modal-dialog">
          <div class="modal-content">
            <div class="modal-header">
              <span>04/08/2016</span>
              <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            </div>
            <div class="modal-body">
              <img id="large_poster" src="http://i.imgur.com/B3nAM6x.jpg?1" alt="large show poster" />
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="fake-footer">
    <div class="foot-sticker">
      <div class="row">
        <div class="col-xs-12 img-responsive">
          <img src="http://i.imgur.com/pwaJbcJ.jpg" alt="mason cos horizontal" />
        </div>
      </div>
    </div>
  </div>
  <div id="shpadoinkle-section" class="row shpadoinkle">
    <div class="col-md-4 col-md-offset-4">
    </div>
  </div>
  <hr>
  <div id="contact-section" class="row contact">
    <div class="row">
      <div class="col-sm-4 col-sm-offset-4 name">
        <h2>Mason Cos</h2>
        <p>
          Chicago, IL
        </p>
        <h3>
          <a href="mailto:masoncosband@gmail.com">
            masoncosband@gmail.com
          </a>
        </h3>
      </div>
    </div>
    <div class="row logos">
      <div class="col-sm-1 col-sm-offset-4">
        <a target="_blank" href="https://www.facebook.com/MasonCosMusic"><%= image_tag 'FB-f-Logo__white_50.png' %></a>
      </div>
      <div class="col-sm-2">
        <a target="_blank" href="https://soundcloud.com/mason-cos"><%= image_tag 'sc_sbs_white_192x24' %></a>
      </div>
      <div class="col-sm-1">
        <a target="_blank" href="https://github.com/MasonCos"><%= image_tag 'GitHub-Mark-64px.png' %></a>
      </div>
    </div>
  </div>

</div>
